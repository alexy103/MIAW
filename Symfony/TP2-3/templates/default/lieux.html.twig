{% extends 'base.html.twig' %}

{% block body %}
    <main>
        <table>
            <thead>
            <tr>
                <th>Nom</th>
                <th>Adresse</th>
                <th>Code Postal</th>
                <th>Ville</th>
            </tr>
            </thead>
            <tbody>
            {% for lieu in lieux %}
                <tr>
                    <td>{{ lieu.nom }}</td>
                    <td>{{ lieu.adresse }}</td>
                    <td>{{ lieu.codePostal }}</td>
                    <td>{{ lieu.ville }}</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </main>
{% endblock %}
